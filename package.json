{
  "name": "contrast",
  "version": "1.0.0",
  "description": "build out of Codewell Contrast challenge",
  "scripts": {
    "css:dev": "postcss src/main.scss --output public/css/main.css --env development --verbose",
    "css:watch": "postcss src/main.scss --output public/css/main.css --env development --verbose --watch",
    "css:build": "postcss src/main.scss --output public/css/main.css --env production --verbose"
  },
  "browserslist": [
    "> 2%, last 2 versions, firefox esr, not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigbuckler/postcss-demo.git"
  },
  "author": "Andrew J Hughes",
  "homepage": "https://github.com/ajhughesdev/contrast.git",
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.11",
    "postcss": "^8.3.11",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-assets": "^6.0.0",
    "postcss-cli": "^8.3.1",
    "postcss-map-get": "^0.2.9",
    "postcss-nested": "^5.0.6",
    "postcss-scss": "^3.0.5",
    "postcss-sort-media-queries": "^3.12.13"
  }
}
